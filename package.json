{
  "name": "cra218",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:unit": "react-scripts test --testPathPattern='(?<!snapshot\\.)test\\.js'",
    "test:snapshots": "react-scripts test --testPathPattern=snapshot\\.test\\.js$",
    "eject": "react-scripts eject",
    "test:cypress": "start-server-and-test start http://localhost:3000 cypress",
    "cypress": "cypress run",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "coveralls": "^3.0.3",
    "cypress": "^3.2.0",
    "jest-dom": "^3.1.3",
    "react-test-renderer": "^16.8.5",
    "react-testing-library": "^6.0.2",
    "start-server-and-test": "^1.7.13"
  }
}
